{
  "scripts": {
    "start": "react-scripts start",
    "test": "jest --ci --reporters=default --reporters=jest-junit"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.x.x",
    "@babel/preset-react": "^7.x.x",
    "jest": "^27.5.1",
    "jest-junit": "^16.0.0",
    "babel-jest": "^27.5.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testEnvironment": "jsdom",
